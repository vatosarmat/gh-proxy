{
  "name": "gh-proxy",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "http-link-header": "^1.0.2",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "object.omit": "^3.0.0",
    "object.pick": "^1.3.0"
  },
  "engines": {
    "node": "12.14"
  },
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "NODE_ENV=production node ./build/index.js",
    "test": "NODE_ENV=test mocha",
    "test:coverage": "nyc yarn test",
    "test:coverage_report": "yarn test:coverage && nyc report --reporter=text-lcov | coveralls",
    "cf": "ts-node ./createFixtures.ts"
  },
  "devDependencies": {
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/http-link-header": "^1.0.1",
    "@types/mocha": "^5.2.7",
    "@types/morgan": "^1.7.37",
    "@types/nock": "^11.1.0",
    "@types/node-fetch": "^2.5.4",
    "@types/object.omit": "^3.0.0",
    "@types/object.pick": "^1.3.0",
    "@types/supertest": "^2.0.8",
    "coveralls": "^3.0.9",
    "mocha": "^6.2.0",
    "nock": "^11.7.0",
    "nyc": "^15.0.0",
    "supertest": "^4.0.2",
    "ts-node-dev": "^1.0.0-pre.42",
    "typescript": "^3.5.3"
  }
}
